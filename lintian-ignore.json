{
    "<package>": [ "list of lintian error keys" ],
    "all": [ "out-of-date-standards-version",
             "ancient-standards-version",
             "binary-without-manpage",
             "hardening-no-fortify-functions",
             "setuid-binary",
             "package-has-long-file-name",
             "icon-size-and-directory-name-mismatch",
             "wrong-name-for-upstream-changelog" ],
    "kdepim-runtime": [ "image-file-in-usr-lib",
                        "package-name-doesnt-match-sonames",
                        "executable-not-elf-or-script" ],
    "kanagram": [ "package-name-doesnt-match-sonames" ],
    "kdepim": [ "image-file-in-usr-lib",
                "korganizer: package-name-doesnt-match-sonames", 
                "executable-not-elf-or-script usr/share/autostart/korgac.desktop", 
                "executable-not-elf-or-script usr/share/autostart/kalarm.autostart.desktop" ],
    "kgoldrunner": [ "unusual-interpreter",
                     "script-not-executable" ],
    "khangman": [ "package-name-doesnt-match-sonames" ],
    "kolf": [ "package-name-doesnt-match-sonames" ],
    "nepomuk-core": [ "nepomuk-core: package-name-doesnt-match-sonames",
                      "nepomuk-core: shlib-without-versioned-soname" ],
    "kde-workspace": [ "kdm: manpage-has-errors-from-man",
                       "kde-style-oxygen: package-name-doesnt-match-sonames",
                       "kde-workspace-bin: package-name-doesnt-match-sonames" ]
}
